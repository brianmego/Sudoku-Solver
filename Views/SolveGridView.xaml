<UserControl x:Class="Sudoku_Solver.Views.SolveGridView"
             xmlns:local="clr-namespace:Sudoku_Solver.ViewModels"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d"
             Height="{Binding Path=SolveGridHeight}"
             Width="{Binding Path=SolveGridWidth}">
    <UserControl.DataContext>
        <local:SolveGridViewModel />
    </UserControl.DataContext>
    <Grid Name="grdGameBoard" />
</UserControl>
